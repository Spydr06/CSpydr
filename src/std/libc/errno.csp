# a basic implementation of the libc errno.h header for the CSpydr programming language

# link with the gnulibc standard library
import "link.csp";

macro EDOM { 1 }
macro EILSEQ { 1 }
macro ERANGE { 1 }

namespace libc {
extern "C" fn __errno_location(): &i32;
}
macro errno() { (*__errno_location()) }