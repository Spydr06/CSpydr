import "regex.csp";

fn main(): i32 
{
    using std;

    let pattern: &std::Regex = regex::compile("[Hh]ello [Ww]orld\\s*[!]?");
    let r = regex::match_pattern(pattern, "ahem.. 'hello world !' ..");
    if r.index != -1 {
        io::printf("matches\n");
    }
    io::printf("matches %i with length %i\n", r.index, r.length);
    regex::print(pattern);

    <- 0;
}