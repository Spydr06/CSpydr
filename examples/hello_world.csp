import "libc/wchar.csp";
import "libc/stdio.csp";

namespace x {
    fn puts(str: &char) {
        puts(str);
    }
}

type data: struct {
    msg: &char,
    exit: i8
};

let program_data: data;

fn main(argc: i32, argv: &&char): i32 {
    program_data = {"Hello World", 0x00}: data;

    x::puts((&program_data).msg);

    <- 0;
}