import "file.csp";
import "io.csp";

macro PATH {
    "examples/assets/test.txt"
}

fn main(): i32 {
    using std;

    with f = file::open(PATH!, file::READ) {
        let buffer = file::read_all(f);
        io::puts(buffer);
        string::free(buffer);
    }
    else
        io::eputs("Error opening " PATH!);
     
    <- 0;
}
