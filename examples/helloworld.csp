import "libc/libc.csp";

type Foo: struct {
    a: i32,
    b: &char,
    c: i64, 
    d: i64
};

fn gen_foo(): Foo {
    <- Foo :: {2, "hi", 0: i64, 0: i64};
}

fn add(a: i64, b: i64): i64 ret a + b;

fn main(argc: i32, argv: &&char): i32
{

    let foo = gen_foo();
    printf("foo.a = %d\n", foo.a);

    <- 0;
}