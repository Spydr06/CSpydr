import "libc/libc.csp";

macro range(low, max) 
{
    let i: i64 = low; i < max; i++
}

fn print_iarr(n: u64, a: i32[])
{
    printf("[");
    for range!(0, n); {
        printf("%d", a[i]);
        if n - i > 1
            printf(", ");
    }
    printf("]\n");
}

fn main(argc: i32, argv: &&char): i32
{
    print_iarr(5, [1, 2, 3, 4, 5]);

    <- 0;
}