import "libc/stdio.csp";
import "vec.csp";

macro println(string) { printf("%s\n", string) }

fn main(argc: i32, argv: &&char): i32
{
    println!("Hello, World!");

    let my_vec = vec![i32];

    vec_add!(&my_vec, 3);
    vec_insert!(&my_vec, 0, 2);
    vec_remove!(my_vec, 0);

    printf("%d %d\n", 0, my_vec[0]);

    std::vec::free(my_vec);

    <- 0;
}