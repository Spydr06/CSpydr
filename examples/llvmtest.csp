# a program to test the llvm frontend
import "libc/time.csp";
import "libc/wchar.csp";
import "libc/stdio.csp";
import "libc/math.csp";

fn main(): i32 {
    let rawtime: time_t;
    let timeinfo: &struct tm;

    time(&rawtime);
    timeinfo = localtime(&rawtime); 

    puts(asctime(timeinfo));

    <- 0;
}