# fibonacci.csp
import "str.csp";
import "io.csp";

fn fib(n: i32): i32
{
    if n <= 1 {
        <- n;
    }

    <- fib(n - 1) + fib(n - 2);
}

fn main(argc: i32, argv: &&char): i32
{
    let n: i32 = 10;
    let str: char[100];

    std::c_str::from_int(fib(n), str, 10);
    std::io::puts(str);

    <- 0;
}