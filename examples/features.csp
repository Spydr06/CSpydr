#import "io.csp";
import "libc/stdio.csp";
import "str.csp";
import "random.csp";

fn main(): i32 {
    using std, std::c_str;
    
    let token = strtok("please,separate,this,by,comma!", ",");

    while token != nil {
        printf("%s\n", token);
        token = strtok(nil, ",");
    }

    printf("<END>\n");

    for let i = 0; i < 10; i++;
        printf("%d\n", random::rand());
    
    <- 0;
}