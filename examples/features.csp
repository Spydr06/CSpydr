import "io.csp";
import "str.csp";

fn main(): i32 {    
    let x = 0;

    match (type) typeof x {
        i32 => {
            std::io::write(stdout!, "x is of type i32\n");
        }

        _ => ret 1;
    }

    if (type) (typeof x) < i32 {
        std::io::write(stdout!, "x is of type i32\n");
    }

    <- 0;
}