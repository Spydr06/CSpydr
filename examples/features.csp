#import "io.csp";
import "libc/stdio.csp";
import "utils.csp";
import "math.csp";
import "option.csp";

fn va_test(a: i32, list: ...)
{
    let x = va_arg!(list, i32);
    printf("%d\n", x);

    let y = va_arg!(list, f32);
    printf("%f\n", y: f64);
}

fn main(): i32 {
    va_test(1, 2, 3.1415);

    <- 0;
}