#import "io.csp";
import "libc/stdio.csp";
import "str.csp";

fn main(): i32 {
    using std::c_str;
    
    let str = "please,separate,this,by,comma!\n";
    let token = strtok(str, ",");


    while token != nil {
        printf("%s\n", token);
        token = strtok(nil, ",");
    }

    printf("<END>\n");

    <- 0;
}