import "std.csp";

type X: enum {
    _1,
    _2,
    _3 = 41,
    _4 # should be 42
};

fn main(): i32 {
    using std;

    io::printf("%i\n", X::_4);

    process::at_exit(|code: i32| => std::io::printf("exit with code %i\n", code));
    rec(3);
    <- 0;
}

fn rec(a: i32): i32 {
    <- if a == 0 => 0 else rec(a - 1);
}

[no_return("main")]